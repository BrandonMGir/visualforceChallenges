<apex:page controller="CustomAccountContactController">
    <apex:pageBlock>
        <apex:pageBlockSection columns="1">
            <apex:pageBlockTable value="{!data}" var="item">
                <apex:column headerValue="Account">
                    <apex:outputText value="{!item.account.name}" />
                </apex:column>
                <apex:column headerValue="Contacts">
                    <apex:outputText value="{!item.contacts}" />
                </apex:column>
            </apex:pageBlockTable>
        </apex:pageBlockSection>
    </apex:pageBlock>
</apex:page>